{
	"name": "yippy/flarum-ext-auth-ldap",
	"description": "Allow users to log in through LDAP.",
	"type": "flarum-extension",
	"keywords": [
		"flarum",
		"authentication",
		"ldap"
	],
	"license": "MIT",
	"funding": [
		{
			"type": "other",
			"url": "https://www.buymeacoffee.com/yippy"
		}
	],
	"authors": [{
		"name": "Yippy",
		"email": "yippym@gmail.com",
		"role": "Developer"
	}],
	"support": {
		"issues": "https://github.com/yippy/flarum-ext-auth-ldap/issues",
		"source": "https://github.com/yippy/flarum-ext-auth-ldap",
		"forum": "https://discuss.flarum.org/d/34414-ldap-login-extension-with-multiple-ldap-server-support"
	},
	"require": {
		"flarum/core": "^2.0",
		"directorytree/ldaprecord": "v2.11.1"
	},
	"autoload": {
		"psr-4": {
			"Yippy\\Flarum\\Auth\\LDAP\\": "src/"
		}
	},
	"extra": {
		"flarum-extension": {
			"title": "LDAP login",
			"icon": {
				"name": "far fa-address-book",
				"backgroundColor": "#b15959",
				"color": "#fff"
			}
		}
	}
}